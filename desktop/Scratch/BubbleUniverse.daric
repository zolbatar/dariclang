REM Thanks to ZXDunny for the original code

MODE -1,-1,BANKED
n=250:r=PI/2/235
x=0.0:t=0.0:sz=300.0
u=0.0:v=0.0
ORIGIN SWIDTH()/2,SHEIGHT()/2
REPEAT
    FOR i=0 TO n
        FOR j=0 TO n
            u=SIN(i+v)+SIN(r*i+x)
            v=COS(i+v)+COS(r*i+x)
            x=u+t
            FG i,j,(i+j)/2
            PLOT u*sz,v*sz
        NEXT
    NEXT
    t=t+0.0025
    FLIP
UNTIL FALSE